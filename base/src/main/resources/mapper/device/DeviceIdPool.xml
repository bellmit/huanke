<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.huanke.iot.base.dao.device.DeviceIdPoolMapper">

    <sql id="tableName">
        t_deviceid_pool
    </sql>
    <!-- 插入一条记录 -->
    <insert id="insert" useGeneratedKeys="true" keyProperty="id" parameterType="com.huanke.iot.base.po.device.DeviceIdPoolPo">
        INSERT INTO <include refid="tableName"/>
        (
        customerId,
        deviceId,
        devicelicence,
        status,
        createTime
        )
        VALUES (
        #{customerId},
        #{deviceId},
        #{devicelicence},
        #{status},
        #{createTime}
        )
    </insert>


</mapper>