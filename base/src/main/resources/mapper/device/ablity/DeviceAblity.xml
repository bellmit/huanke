<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.huanke.iot.base.dao.impl.device.ablity.DeviceAblityMapper">

    <sql id="tableName">
        t_device_ablity
    </sql>
    <!-- 插入一条记录 -->
    <insert id="insert" useGeneratedKeys="true" keyProperty="id" parameterType="com.huanke.iot.base.po.device.alibity.DeviceAblityPo">
        INSERT INTO <include refid="tableName"/>
        (
        ablityName,
        dirValue,
        createTime
        )
        VALUES (
        #{ablityName},
        #{dirValue},
        #{createTime}
        )
    </insert>

</mapper>